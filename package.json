{
	"name": "svu",
	"description": "Svultimate Svelte(Kit) Svutilities",
	"version": "0.3.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nikolai-cc/svu.git"
	},
	"author": "nikolai-cc (https://nikolai.cc)",
	"license": "MIT",
	"homepage": "svu.vercel.app",
	"bugs": {
		"url": "https://github.com/nikolai-cc/svu/issues"
	},
	"scripts": {
		"test": "pnpm run test:vitest && pnpm run test:playwright",
		"test:clean": "rm -rf ./test/playwright/.cache",
		"test:setup": "tsm ./test/setup.ts",
		"test:vitest": "pnpm run test:setup && vitest run -c test/vitest.config.ts",
		"test:playwright": "pnpm run test:setup && playwright test -c test/playwright.config.ts",
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/experimental-ct-svelte": "^1.35.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.20.5",
		"@sveltejs/package": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.60.1",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fast-glob": "^3.2.12",
		"mdsvex": "^0.11.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "4.0.0",
		"svelte-check": "^3.4.4",
		"svelte-preprocess": "^5.0.4",
		"svelte2tsx": "^0.6.16",
		"tslib": "^2.6.0",
		"tsm": "^2.3.0",
		"typescript": "^5.1.5",
		"vite": "^4.3.9",
		"vitest": "^0.32.2"
	},
	"module": "./index.js",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"svelte": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./action": {
			"svelte": "./dist/action/index.js",
			"types": "./dist/action/index.d.ts"
		},
		"./app/": {
			"svelte": "./dist/app/index.js",
			"types": "./dist/app/index.d.ts"
		},
		"./components/": {
			"svelte": "./dist/components/index.js",
			"types": "./dist/components/index.d.ts"
		},
		"./store/": {
			"svelte": "./dist/store/index.js",
			"types": "./dist/store/index.d.ts"
		},
		"./transition/": {
			"svelte": "./dist/transition/index.js",
			"types": "./dist/transition/index.d.ts"
		},
		"./svelte": {
			"svelte": "./dist/svelte/index.js",
			"types": "./dist/svelte/index.d.ts"
		},
		"./svelte/action": {
			"svelte": "./dist/svelte/action/index.js",
			"types": "./dist/svelte/action/index.d.ts"
		},
		"./svelte/app/": {
			"svelte": "./dist/svelte/app/index.js",
			"types": "./dist/svelte/app/index.d.ts"
		},
		"./svelte/components/": {
			"svelte": "./dist/svelte/components/index.js",
			"types": "./dist/svelte/components/index.d.ts"
		},
		"./svelte/store/": {
			"svelte": "./dist/svelte/store/index.js",
			"types": "./dist/svelte/store/index.d.ts"
		},
		"./svelte/transition/": {
			"svelte": "./dist/svelte/transition/index.js",
			"types": "./dist/svelte/transition/index.d.ts"
		}
	},
	"type": "module",
	"packageManager": "pnpm@8.6.5"
}