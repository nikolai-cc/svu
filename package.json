{
  "name": "rewrite",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./action": {
      "svelte": "./dist/action/index.js",
      "types": "./dist/action/index.d.ts"
    },
    "./app/": {
      "svelte": "./dist/app/index.js",
      "types": "./dist/app/index.d.ts"
    },
    "./components/": {
      "svelte": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./store/": {
      "svelte": "./dist/store/index.js",
      "types": "./dist/store/index.d.ts"
    },
    "./transition/": {
      "svelte": "./dist/transition/index.js",
      "types": "./dist/transition/index.d.ts"
    },
    "./svelte": {
      "svelte": "./dist/svelte/index.js",
      "types": "./dist/svelte/index.d.ts"
    },
    "./svelte/action": {
      "svelte": "./dist/svelte/action/index.js",
      "types": "./dist/svelte/action/index.d.ts"
    },
    "./svelte/app/": {
      "svelte": "./dist/svelte/app/index.js",
      "types": "./dist/svelte/app/index.d.ts"
    },
    "./svelte/components/": {
      "svelte": "./dist/svelte/components/index.js",
      "types": "./dist/svelte/components/index.d.ts"
    },
    "./svelte/store/": {
      "svelte": "./dist/svelte/store/index.js",
      "types": "./dist/svelte/store/index.d.ts"
    },
    "./svelte/transition/": {
      "svelte": "./dist/svelte/transition/index.js",
      "types": "./dist/svelte/transition/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^4.2.10"
  },
  "devDependencies": {
    "@fontsource-variable/recursive": "^5.0.17",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/package": "^2.3.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "8.56.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "changesets": "^1.0.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "publint": "^0.2.7",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
