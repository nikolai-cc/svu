{
	"name": "svu",
	"description": "Svultimate Svelte(Kit) Svutilities",
	"version": "0.3.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nikolai-cc/svu.git"
	},
	"author": "nikolai-cc (https://nikolai.cc)",
	"license": "MIT",
	"homepage": "svu.vercel.app",
	"bugs": {
		"url": "https://github.com/nikolai-cc/svu/issues"
	},
	"scripts": {
		"test": "npm run test:vitest && npm run test:playwright",
		"test:clean": "rm -rf ./test/playwright/.cache",
		"test:setup": "tsm ./test/setup.ts",
		"test:vitest": "npm run test:setup && vitest run -c test/vitest.config.ts",
		"test:playwright": "npm run test:setup && playwright test -c test/playwright.config.ts",
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/experimental-ct-svelte": "^1.34.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.20.0",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fast-glob": "^3.2.12",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"svelte2tsx": "^0.6.15",
		"tslib": "^2.5.2",
		"tsm": "^2.3.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.31.4"
	},
	"module": "./index.js",
	"exports": {
		".": "./index.js",
		"./action": "./action/index.js",
		"./app": "./app/index.js",
		"./client": "./client/index.js",
		"./components": "./components/index.js",
		"./store": "./store/index.js",
		"./transition": "./transition/index.js",
		"./svelte/action": "./svelte/action/index.js",
		"./svelte/app": "./svelte/app/index.js",
		"./svelte/client": "./svelte/client/index.js",
		"./svelte/components": "./svelte/components/index.js",
		"./svelte/store": "./svelte/store/index.js",
		"./svelte/transition": "./svelte/transition/index.js"
	},
	"type": "module"
}
